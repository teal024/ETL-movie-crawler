    var b=CSMReqs[marker];
    if(b.c == 0){
        if(window.ue && typeof ue.tag === 'function') {
            ue.tag(customtag);
        }
    }
};
;(function(_onerror){
  var old_error_handler = _onerror;
  var attributionMap = {
          "BrowserAddon":{
            logLevel: "ERROR",
            files:[
                /^res:\/\//, 
                /^resource:\/\//, /^chrome:\/\//, 
                /^chrome-extension:\/\//, /^extensions\//, 
                /^file:\/\/\//, /^chrome\/RendererExtensionBindings/, 
